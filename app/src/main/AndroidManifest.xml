<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SongsActivity"
            android:label="@string/category_songs">

        </activity>
        <activity
            android:name=".PlaylistActivity"
            android:label="@string/category_playlist">

        </activity>
        <activity
            android:name=".NowPlayingActivity"
            android:label="@string/category_now_playing">

        </activity>
        <activity
            android:name=".AlbumActivity"
            android:label="@string/category_albums">

        </activity>
        <activity
            android:name=".ArtistActivity"
            android:label="@string/category_artists">

        </activity>
    </application>

</manifest>